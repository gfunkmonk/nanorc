syntax "crontab" "crontab*"
color brightwhite "[0-9]"
color bold,rosy "\*"
color bold,sky "[[:space:]]/.*$"
color bold,lightmagenta "\"[^\"]*\""
color bold,lightmagenta "'([^']*)'"
color bold,slate "(&|>)"
color italic,lightyellow "#.*$"
color bold,lightyellow "https?:\/\/(www\.)?[-a-zA-Z0-9@:%._\+~#=]{1,256}\.[a-zA-Z0-9()]{1,6}\b([-a-zA-Z0-9()@:%_\+.~#?&//=]*)"
