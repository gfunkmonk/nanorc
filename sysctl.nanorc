syntax "sysctl" "sysctl.conf$"
color bold,mauve "^[[:space:]]*[.0-9A-Z_]*.*$"
color bold,sand "= 1"
color bold,sand "=1"
color bold,slate "=0"
color bold,slate "= 0"
color bold,peach "\="
color italic,beet "#.*$"

syntax "^/etc/sysctl\.d/[^/]+(\.|/)conf$" "^/etc/sysctl\.d/[^/]+\.conf$"
color bold,lagoon "^[[:space:]]*[.0-9A-Z_]*.*$"
color bold,sand "= 1"
color bold,sand "=1"
color bold,slate "= 0"
color bold,slate "=0"
color bold,peach "\="
color italic,sea "#.*$"
